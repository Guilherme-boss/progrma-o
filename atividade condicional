//Faça um programa que leia um número inteiro e verifique se esse número é par ou ímpar.
int main(){
int main(int argc, char *argv[]) {
	int n1,n2;
	printf("Digite o nuemero\n");
	scanf("%i",&n1);
	printf("digite o numero\n");
	scanf("%i",&n2);
	
	if (n1>n2){
	printf("o numero par eh:%i\n",n1);
	}
	else{
	printf("o numero eh impar!%i\n",n2);
		
	}
	return 0;
}	
	
	
	
	
  
