#include<stdio.h>
int main(){
 double cd[5], cd10[5], cdX[5], porcentagem;
 int i;

 for(i=0;i<5;i++){
 	printf("digite o valor do cd: ");
 	scanf("%lf", &cd[i]);
 }
 for(i=0;i<5;i++){
 	cd10[i] = cd[i] + cd[i] * 10/100;
 }
 printf("vendedor, qual a porcentaguem em cada cd: ");
 scanf("%lf", &porcentagem);

 for(i=0;i<5;i++){
 	cdX[i] = cd[i] + cd[i] * (porcentagem/100);
 }

 for(i=0;i<5;i++){
 	printf("O valor do cd e %.2lf\n", cd[i]);
 	printf("O valor do cd10 e %.2fl\n", cd10[i]);
 	printf("o valor do cdX e %.2fl\n", cdX[i]);
	
	
	//faça um programa que leia 2 numeros e mostre qual e o maior
	#include<stdio.h>
int main(){
	int valorA,valorB;
	
	printf("digite o primeiro numero: ");
	scanf("%d",&valorA);
	printf("digite o segundo numero");
	scanf("%d",&valorB);
	
	if(valorA>valorB){
		printf("%d",valorA);
	}
    else
	{
    printf("%d",valorB);
	}
	
	
	
}



//faça um programa qu leia um numero, caso ele seja positivo
//apresenta o quadra e a raiz quadrada do numero digitado 

#include<stdio.h>
#include<math.h>
int main(){
	int numero,quadrado;
	float raiz;
	printf("digite o numero:");
	scanf("%d",&numero);
	if (numero > 0)
	{
		quadrado = pow(numero,2);
		raiz = sqrt (numero);
		printf("numero digitado %d\nquadrado %d\nraiz %f\n",numero,quadrado,raiz);
	}
	
		
}



#include <stdio.h>
Escreva um programa que leia o nome do funcionário, o sexo (F ou M), o número de
matrícula do funcionário, o número de horas trabalhadas, o valor que recebe por hora
e calcule o salário desse funcionário. A seguir, mostre todos os dados na tela, os dados
de entrada e os dados de saída.

int main() {
char nome[50],sexo[50];
float matricula, horastrabalhadas,valorporhora,salario;
printf("digite seu nome\n");
scanf("%s", nome);
printf("digite seu sexo");
scanf("%s", sexo);
    
printf("Digite o numero da matricula\n");
scanf("%f",&matricula);
printf("Digite as horas trabalhadas\n");  scanf("%f",&horastrabalhadas);
printf("Digite o valor que recebe por hora\n");
scanf("%f",&valorporhora);

salario = horastrabalhadas * valorporhora;

printf("nome: %s \n",nome);
printf("sexo: %s\n",nome);
printf("matricula: %.2f",matricula);
printf("horas trabalhadas: %.2f",horastrabalhadas);
printf("valor que recebe por horas %.2f",valorporhora);
printf("salario eh: %.2f",salario);


