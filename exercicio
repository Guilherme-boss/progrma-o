#include <stdio.h>
//Construa um algoritmo que a partir da leitura de dois números forneça o resto e o quociente da divisão do primeiro pelo segundo número.
int main() {
int valorA,valorB,restodadivisao,valordadivisao;
printf("Digite o valor a:\n");
scanf("%i",&valorA);

printf("digite o valor b:\n");
scanf("%i",valorB);

restodadivisao = valorA % valorB;
valordadivisao = valorA % valorB;

printf("o resto da divisao eh:%.1i\n",restodadivisao);
printf("o valor da divisao eh:%.1i\n",valordadivisao);
  
return 0;
}
