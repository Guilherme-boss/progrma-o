int main() {
int metros,rolos,metrosavulsos;
printf("Digite a quantidade de metros:");
scanf("%i",&metros);

rolos = metros / 50;
metrosavulsos = metros % 50;

printf("rolos eh:%i\n",rolos);
printf("metros avulsos eh:%i",metrosavulsos);

  



return 0;
